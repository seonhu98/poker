<Window x:Class="pokerProject.Cam_"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pokerProject"
        Loaded="windows_loaded"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="650">
    <Grid OpacityMask="#FF8DA9FF" Background="#FFC7E0FC">
        <Image Name="Cam_1" OpacityMask="#FF060606" Margin="60,49,60,94" Height="300" Width="500"/>
        <!--둥근 테두리 버튼 (200)-->
        <Button x:Name="upload" Content="upload" Margin="509,277,29,45" BorderBrush="{x:Null}" Opacity="0.7" Click="upload_Click" Foreground="#FF634B4B" FontSize="24" FontWeight="Bold" FontFamily="Segoe Print" Background="White">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="200"/>
                </Style>
            </Button.Resources>
        </Button>
    </Grid>
</Window>